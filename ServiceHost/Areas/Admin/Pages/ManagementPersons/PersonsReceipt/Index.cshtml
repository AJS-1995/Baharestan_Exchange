                            <thead class="table-Primary">
                                <tr>
                                    <th class="text-center">تاریخ</th>
                                    <th class="text-center">مقدار فروش</th>
                                    <th class="text-center">قیمت</th>
                                    <th class="text-center">مقدار خرید</th>
                                    <th class="text-center">عملیات</th>
                                </tr>
