                var postData = {
                    'moneyId': moneyId,
                    'PersonsId': PersonsId,
                };
                $.ajax({
                    ccontentType: 'application/x-www-form-urlencoded',
                    dataType: 'json',
                    type: "GET",
                    url: "@Url.Page("/ManagementPersons/PersonsMoneyExchange/Index", "Money")",
                    data: postData,
                    success: function (date) {
                        document.getElementById('Amount_One').value = date;
                        Plus();
                    },
                });
            }
        }
        function Plus() {
            var amount_One = $("#Amount_One").val();
            var price = $("#Price").val();
            var multiplication = $("#Multiplication");
            var division = $("#Division");
            let a = parseFloat(amount_One);
            let b = parseFloat(price);
            if (Division.checked == true) {
                var total = a / b;
            } else {
                var total = a * b;
            }
            if (total == Infinity) {
                total = 0;
            }
            if (isNaN(total)) {
                total = 0;
            }
            document.getElementById('Amount_Two').value = total.toFixed(2);
        }
    </script>

